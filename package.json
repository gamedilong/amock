{
  "name": "amock",
  "displayName": "amock",
  "publisher": "gamedilong",
  "description": "It's a lightweight mock server plugin based on express. No more than 5 minutes of skilled use.",
  "version": "0.0.1",
  "keywords": [
    "Mock",
    "Express",
    "Server",
    "LightWeight",
    "Easy",
    "VSCode Plugin",
    "vscode"
  ],
  "bugs": {
    "url": "https://github.com/gamedilong/amock/issues",
    "email": "1104238614@qq.com"
  },
  "homepage": "https://github.com/gamedilong/amock",
  "repository": {
    "type": "git",
    "url": "https://github.com/gamedilong/amock.git"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:amock",
    "onCommand:extension.amockStart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "amock",
          "name": "amock"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.amockStart",
        "title": "AMock Start",
        "icon": {
          "light": "resources/light/run.svg",
          "dark": "resources/dark/run.svg"
        }
      },
      {
        "command": "extension.amockReload",
        "title": "AMock Reload",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "extension.amockStop",
        "title": "AMock Stop",
        "icon": {
          "light": "resources/light/stop.svg",
          "dark": "resources/dark/stop.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "extension.amockStart",
          "when": "view == amock",
          "group": "navigation@1"
        },
        {
          "command": "extension.amockReload",
          "when": "view == amock",
          "group": "navigation@2"
        },
        {
          "command": "extension.amockStop",
          "when": "view == amock",
          "group": "navigation@3"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "express": "^4.18.2"
  }
}
